{
  "name": "ember-goods",
  "version": "1.20.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "github:goods/ember-goods",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "postpublish": "ember ts:clean",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-assembly": "1.2.1",
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-typescript": "^4.0.0",
    "ember-credit-cards": "^1.5.0",
    "ember-css-modules": "^1.6.2",
    "ember-css-modules-sass": "^1.1.0",
    "ember-concurrency": "^2.2.1",
    "ember-get-config": "^0.2.4",
    "ember-moment": "^10.0.0",
    "ember-simple-auth": "^3.0.0",
    "ember-uuid": "^2.1.0",
    "postcss-functions": "^3.0.0",
    "sass": "^1.60.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.0",
    "@types/ember-data": "^3.16.3",
    "@types/ember-resolver": "^5.0.9",
    "@types/qunit": "^2.19.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-update": "^0.52.1",
    "ember-data": "^3.15.0",
    "ember-data-resources": "^4.0.4",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
